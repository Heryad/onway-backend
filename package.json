{
  "name": "onway-backend",
  "version": "1.0.0",
  "description": "High-performance e-commerce delivery platform",
  "module": "src/index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "start": "NODE_ENV=production bun src/index.ts",
    "build": "bun build src/index.ts --outdir ./dist --target bun",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "bun src/db/migrate.ts",
    "db:seed": "bun src/db/seed.ts",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "db:drop": "drizzle-kit drop",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.7",
    "@hono/zod-validator": "^0.4.1",
    "@socket.io/redis-adapter": "^8.3.0",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/sharp": "^0.32.0",
    "argon2": "^0.44.0",
    "bullmq": "^5.66.0",
    "dayjs": "^1.11.19",
    "drizzle-orm": "^0.45.1",
    "fluent-ffmpeg": "^2.1.3",
    "hono": "^4.11.1",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "minio": "^8.0.6",
    "nanoid": "^5.0.9",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "postgres": "^3.4.7",
    "redis": "^5.10.0",
    "sharp": "^0.34.5",
    "socket.io": "^4.8.1",
    "typesense": "^2.1.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^25.0.2",
    "@types/jsonwebtoken": "^9.0.7",
    "drizzle-kit": "^0.31.8",
    "typescript": "^5.7.3",
    "eslint": "^9.18.0",
    "prettier": "^3.4.2"
  }
}